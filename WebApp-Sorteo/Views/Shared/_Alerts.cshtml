@using WebApp_Sorteo.Helpers;
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="
https://cdn.jsdelivr.net/npm/toastr@2.1.4/toastr.min.js
">

</script>
@if (TempData[DS.Exitosa] != null)
{
    <script type="text/javascript">
        toastr.success('@TempData[DS.Exitosa]');
    </script>
}
@if (TempData[DS.Info] != null)
{
    <script type="text/javascript">
        toastr.info('@TempData[DS.Info]');
    </script>
}

@if (TempData[DS.Error] != null)
{
    <script type="text/javascript">
        toastr.error('@TempData[DS.Error]', 'Error!');
    </script>
}